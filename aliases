get-pip=python "%appie_ROOT%\base\get-pip.py" $*
updatethis=git pull origin master
update=cd /d "%appie_ROOT%" $T sh "%appie_ROOT%\bin\update.sh" $*

androbugs="%appie_ROOT%\bin\AndroBugs_Framework\androbugs.exe" $*

