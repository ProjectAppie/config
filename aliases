sqlitebrowser="%appie_ROOT%\bin\SQLiteDatabaseBrowser64\sqlitebrowser.exe" -new_console

sqlmap=python "%appie_ROOT%\bin\sqlmap\sqlmap.py" $*

apktool=java -jar "%appie_ROOT%\bin\tools-repo\apktool\apktool.jar" $*
burpsuite=start javaw -jar "%appie_ROOT%\bin\tools-repo\burpsuite\burpsuite.jar" -new_console
jdgui=start javaw -jar "%appie_ROOT%\bin\JD-GUI\jd-gui-1.4.0.jar" $1 -new_console
jadx="%appie_ROOT%\bin\tools-repo\jadx\bin\jadx.bat" $*
jadx-gui="%appie_ROOT%\bin\tools-repo\jadx\bin\jadx-gui.bat" $* -new_console


